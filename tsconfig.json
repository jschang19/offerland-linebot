{
    "compilerOptions": {
      "target": "es2018",
      "baseUrl": "./src",
      "module": "commonjs",
      "esModuleInterop": true,
      "strict": true,
      "outDir": "dist",
      "paths": {
        "@utils/*": ["utils/*"]

      }
    },
    "include": ["src/**/*"],
